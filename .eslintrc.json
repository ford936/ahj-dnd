{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "guard-for-in": "off",
    "class-methods-use-this": "off",
    "max-len": "off",
    "no-unused-expressions": "off"
  }
}
